{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)\\.(js|css|png|jpg|jpeg|svg|webp|avif)$", "headers": { "cache-control": "public, max-age=0, s-maxage=31536000, stale-while-revalidate=86400" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
